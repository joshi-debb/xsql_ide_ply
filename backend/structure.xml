<?xml version="1.0" ?>
	<SCHEMAS>
		
		
		<database name="productos">
			<table name="parsona">
				<atributes>
					<atribute value="nombre" type="TipoDato.NVARCHAR" param="TipoOpciones.NULL"/>
					<atribute value="apellido" type="TipoDato.NVARCHAR" param="TipoOpciones.NOTNULL"/>
					<atribute value="fecha_nac" type="TipoDato.DATE" param="TipoOpciones.PRIMARYKEY"/>
					<atribute value="genero" type="TipoDato.BIT" param="TipoOpciones.NOTNULL"/>
				</atributes>
				<tuples/>
			</table>
		</database>
		
	
	</SCHEMAS>
